{
  "name": "UserMnt",
  "version": "1.0.0",
  "description": "A NestJS project for managing usuarios with MySQL integration.",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "dotenv -e .env.development nest start --watch",
    "start:prod": "dotenv -e .env.production cross-env NODE_ENV=production node dist/main.js",
    "start:test": "dotenv -e .env.test jest",
    "build": "nest build",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "npm install @nestjs/config @nestjs/typeorm typeorm mysql2"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "moment-timezone": "^0.5.48",
    "mysql2": "^3.14.1",
    "or": "^0.2.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^9.0.0",
    "@types/jest": "^26.0.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^4.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
